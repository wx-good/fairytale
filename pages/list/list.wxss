/* pages/list/list.wxss */

.list {
  width: 100%;
  height: 65px;
  border-top: 1px solid #808080;
  border-bottom: 1px solid #808080;
  margin-bottom: 5px;
  line-height: 65px;
  color: #999;
}

.list image {
  height: 65px;
  width: 65px;
  vertical-align: top;
  padding-right: 20px;
}

.user {
  width: 100%;
  height: 60px;
  color: #fff;
  background: #333;
  text-align: center;
  line-height: 60px;
  font-size: 22px;
  position: fixed;
  top: 0;

  
}
.center{
  padding-top: 60px;
}

.s{
  width: 6px;
  height: 1000px;
}